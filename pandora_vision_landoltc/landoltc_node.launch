<launch>
    <group ns="pandora_vision_landoltc">
		
			<node name="landoltc_node" type="landoltc_node" pkg="pandora_vision_landoltc" respawn="False" output="screen" />
			<!-- Add in upper line to enable debugging: launch-prefix="kdbg" /-->
			
			<!-- Package Path /-->
			<param name="/pandora_vision/packagepath" value="$(find pandora_vision_landoltc)" />

            <!-- Locate general_params_launcher /-->
			<include file="$(find pandora_vision_launchers)/launch/general_params.launch" />	

    </group>
</launch>
